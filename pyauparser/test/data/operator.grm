! ====================================================================
! Properties
! ====================================================================

"Name"              = 'Operator Example'
"Author"            = 'Esun Kim'
"Version"           = '1.0'
"About"             = 'Test Data'
"Start Symbol"      = <E>

! ====================================================================
! Terminals
! ====================================================================

{Digi9} = {Digit} - ['0']
Num     = '0' | {Digi9}{Digit}*

! ====================================================================
! Rules
! ====================================================================

<E>   ::= <E> '+' <M> 
       |  <E> '-' <M> 
       |  <M> 

<M>   ::= <M> '*' <N> 
       |  <M> '/' <N> 
       |  <N> 

<N>   ::= '-' <V> 
       |  <V> 

<V>   ::= Num
       |  '(' <E> ')'
